import os
import numpy as np
import pandas as pd

path = "/Users/attiliocastano/Documents/GitHub/FinanceErdosProj/country_data/"

DF_tess = pd.read_csv(os.path.join(path,'Tesseract_DF_with_countries.csv'))
DF_pdf = pd.read_csv(os.path.join(path,'PyMuPdf_DF_with_countries.csv'))

count = 0
for x in DF_pdf.loc[:, 'countries']:
    if x == '[[], []]':
        count = count + 1
print(count)
